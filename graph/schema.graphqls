# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type Bank {
  id: ID!
  name: String!
  country: String!
  website: String
  logoURL: String
  offers: [Offer!]!
  currencyRates: [CurrencyRate!]!
}

type Offer {
  id: ID!
  type: String!
  description: String!
  link: String
}

type CurrencyRate {
  id: ID!
  currency: String!
  rate: Float!
}

type Query {
  banks: [Bank!]!
}

type Mutation {
  createBank(name: String!, country: String!): Bank!
}